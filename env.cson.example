{
  # Badgekit configuration
  # To get the staging secret key, ping k88hudson or echristensen on IRC (irc.mozilla.org)
  badgekitApiUrl: 'https://badgekit-api.mofostaging.net'
  badgekitApiKey: 'master'
  badgekitApiSecret: ''

  # Output GELF-formatted logs, or development logs
  enableGELFLogs: false

  # URL for Login
  loginUrl: 'http://localhost:3000'
  loginUrlWithAuth: 'http://testuser:password@localhost:3000'
  loginEmailUrl: 'http://localhost:1969'

  # Login secret key
  sessionSecret: 'dummy secret value'

  # Login cookie session settings
  cookieDomain: ''
  forceSSL: false

  # What environment webmaker-profile-2 is running in
  nodeEnv: 'development'

  # Warning - everything in this block will be exposed to the front end
  public:

    # URL for Events app
    # Appended to the beginning of outbound event URLs
    eventApp: 'http://localhost:1981'

    # URL for Events Service
    eventService: 'http://localhost:1989'

    # URL for MakeAPI
    makeAPI: 'https://makeapi.webmaker.org'

    # Webmaker badges url
    webmakerBadgesUrl: 'https://webmaker.mofostaging.net/badges'

    # URL for user account settings
    accountSettingsURL: 'http://localhost:3000/account'

    # URL to POST a new badge to the Openbadges Backpack
    backpackURL: 'https://backpack.openbadges.org/issuer/frameless'
}
